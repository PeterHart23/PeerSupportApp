{
  "expo": {
    "name": "PeerSupport",
    "slug": "PeerSupport",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "PeerSupport.peter"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "android": {
      "useNextNotificationsApi": true,
      "package": "PeerSupport.peter",
      "config": {
        "googleSignIn": {
          "apiKey": "PLACE_API_KEY_HERE"
        }
      }
    }
  }
}
